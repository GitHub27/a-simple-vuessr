{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "Vue.js Server Side Render Template with Webpack & Express",
  "main": "index.js",
  "scripts": {
    "dev": "node app",
    "deploy": "cross-env NODE_ENV=production node app",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccforward/vue-ssr.git"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "compression": "^1.7.1",
    "element-ui": "^2.1.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.2",
    "file-stream-rotator": "^0.2.1",
    "jquery": "^3.3.1",
    "lru-cache": "^4.1.1",
    "morgan": "^1.9.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "captchapng": "^0.0.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "vue": "^2.5.13",
    "vue-loader": "^14.0.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  },
  "keywords": [
    "Vue",
    "SSR",
    "Server Side Render"
  ],
  "author": "cc <ccking@foxmail.com>",
  "license": "MIT",
